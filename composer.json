{
    "name": "zeggriim/yousign-webhook-bundle",
    "description": "A Symfony bridge for handling Yousign webhooks via RemoteEvent.",
    "keywords": ["yousign", "webhook", "remote-event", "symfony", "bundle"],
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "symfony/http-foundation": "^6.0|^7.0",
        "symfony/http-kernel": "^6.0|^7.0",
        "symfony/framework-bundle": "^5.4|^6.0|^7.0",
        "symfony/remote-event": "^6.0|^7.0",
        "webmozart/assert": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "Zeggriim\\YousignWebhookBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Zeggriim",
            "email": "lilian.dorazio@hotmail.fr"
        }
    ],
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.82",
        "phpunit/phpunit": "^12.2"
    },
    "autoload-dev": {
        "psr-4": {
            "Zeggriim\\YousignWebhookBundle\\Tests\\": "tests/"
        }
    }
}
